    function initParticles() {
      particlesJS.load('particles-js', '/mfm-landing/particles.json');
      window.addEventListener('scroll', () => {
        const header = document.querySelector('.scroll-blur');
        if (window.scrollY > 100) {
          header.classList.add('back-blur');
        } else {
          header.classList.remove('back-blur');
        }
      });
    }
